{
    "function": {
        "visitors": {
            "build": true,
            "providerPlugin": "awscloudformation",
            "service": "Lambda",
            "dependsOn": [
                {
                    "category": "storage",
                    "resourceName": "checkInStorage",
                    "attributes": [
                        "Name",
                        "Arn"
                    ]
                },
                {
                    "category": "storage",
                    "resourceName": "validationStorage",
                    "attributes": [
                        "Name",
                        "Arn"
                    ]
                }
            ]
        }
    },
    "api": {
        "v1": {
            "service": "API Gateway",
            "providerPlugin": "awscloudformation",
            "dependsOn": [
                {
                    "category": "function",
                    "resourceName": "visitors",
                    "attributes": [
                        "Name",
                        "Arn"
                    ]
                }
            ]
        }
    },
    "storage": {
        "checkInStorage": {
            "service": "DynamoDB",
            "providerPlugin": "awscloudformation"
        },
        "validationStorage": {
            "service": "DynamoDB",
            "providerPlugin": "awscloudformation"
        }
    }
}